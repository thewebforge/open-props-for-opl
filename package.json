  "scripts": {
    "build": "concurrently npm:gen:op npm:gen:shadowdom",
    "test": "ava test/basic.test.cjs",
    "bundle": "concurrently npm:op-lite:* -m 25 && npm run module:js",
    "bundle:pre-edit": "json -I -f package.json -e \"this.unpkg=''\"",
    "bundle:post-edit": "json -I -f package.json -e \"this.unpkg='open-props.min.css'\"",
    "gen:op": "cd build && node props.js '' true",
    "gen:nowhere": "cd build && node props.js '' false",
    "gen:shadowdom": "cd build && node props.js '' false ':host' 'shadow'",
    "gen:prefixed": "cd build && node props.js 'op' true",
    "gen:oxy": "cd build && node props.js 'o' true",
    "module:js": "npm run bundle:pre-edit && microbundle --no-sourcemap && npm run bundle:post-edit",
    "op-lite:all": "postcss src/op-lite.index.css -o ../assets/css/open-props/open-props.op-lite.min.css",
    "op-lite:normalize": "postcss src/extra/normalize.css -o ../assets/css/open-props/normalize.op-lite.min.css",
    "op-lite:buttons": "postcss src/extra/buttons.css -o ../assets/css/open-props/buttons.op-lite.min.css",

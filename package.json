    "op-lite:colors": "postcss src/op-lite.props.colors.css -o ../assets/css/open-props/colors.op-lite.min.css",
    "op-lite:colors-hsl": "postcss src/op-lite.props.colors-hsl.css -o ../assets/css/open-props/colors-hsl.op-lite.min.css",
